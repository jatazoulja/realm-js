{
  "opts": {
    "destination": "docs/output",
    "readme": "README.md",
    "recurse": true,
    "template": "docs/jsdoc-template/template"
  },
  "source": {
    "include": ["docs"],
    "exclude": ["docs/jsdoc-template", "docs/output"]
  },
  "tags": {
    "allowUnknownTags": false
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "default": {
      "outputSourceFiles": false
    }
  }
}
